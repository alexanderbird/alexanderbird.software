<!DOCTYPE html>
<html lang='en'>
  {{> invisible/html-head title=header.title}}
  {{#> atoms/html-body class='pages__blog-index' }}
    {{> invisible/signature }}
    {{> invisible/icon-symbols }}
    {{> organisms/blog-header header }}
    <section class='page__blog-index__posts'>
      {{#each posts}}
        {{> organisms/blog-post-summary this }}
      {{/each}}
    </section>
    <footer class='pages__blog-index__footer'>
      {{> atoms/copyright-footer copyright }}
    </footer>
  {{/atoms/html-body}}
</html>
