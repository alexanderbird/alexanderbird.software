<html lang='en'>
  {{> invisible/html-head}}
  {{#> atoms/html-body}}
    {{> invisible/signature }}
    {{> invisible/css-helper-script }}


  {{> dynamic/icons }}
  <table>
    <thead>
      <tr>
        <th></th>
        {{#each propertySets as |properties|}}
          <th>{{properties.summary}}</th>
        {{/each}}
      </tr>
    </thead>
    <tbody>
      {{#each iconNames as |iconName|}}
        <tr>
          <th>{{iconName}}</th>
          {{#each ../propertySets as |properties|}}
            <td
              style="
                --icon-color-primary: {{properties.color-primary}};
                --icon-color-secondary: {{properties.color-secondary}};
                --icon-color-tertiary: {{properties.color-tertiary}};
                --icon-height: {{properties.height}};
                --icon-width: {{properties.width}};
              "
              >{{> atoms/svg-icon name=iconName }}</td>
          {{/each}}
        </tr>
      {{/each}}
    <tbody>
  </table>

    <script src='/signature.js'></script>
  {{/atoms/html-body}}
</html>
